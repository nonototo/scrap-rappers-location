<ol><li><b><a href="/wiki/Lenny_Curry" title="Lenny Curry">Lenny Curry</a> (<a href="/wiki/Republican_Party_(United_States)" title="Republican Party (United States)">R</a>)</b><br />(<a href="/wiki/Jacksonville,_Florida" title="Jacksonville, Florida">Jacksonville</a>)</li>
<li><b><a href="/wiki/Francis_X._Suarez" title="Francis X. Suarez">Francis Suarez</a> (<a href="/wiki/Republican_Party_(United_States)" title="Republican Party (United States)">R</a>)</b><br />(<a href="/wiki/Miami" title="Miami">Miami</a>)</li>
<li><b><a href="/wiki/Bob_Buckhorn" title="Bob Buckhorn">Bob Buckhorn</a> (<a href="/wiki/Democratic_Party_(United_States)" title="Democratic Party (United States)">D</a>)</b><br />(<a class="mw-selflink selflink">Tampa</a>)</li>
<li><b><a href="/wiki/Buddy_Dyer" title="Buddy Dyer">Buddy Dyer</a> (<a href="/wiki/Democratic_Party_(United_States)" title="Democratic Party (United States)">D</a>)</b><br />(<a href="/wiki/Orlando,_Florida" title="Orlando, Florida">Orlando</a>)</li>
<li><b><a href="/wiki/Rick_Kriseman" title="Rick Kriseman">Rick Kriseman</a> (<a href="/wiki/Democratic_Party_(United_States)" title="Democratic Party (United States)">D</a>)</b><br />(<a href="/wiki/St._Petersburg,_Florida" title="St. Petersburg, Florida">St. Petersburg</a>)</li>
